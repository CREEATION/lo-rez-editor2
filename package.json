{
  "name": "lo-rez-editor",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write --plugin-search-dir=. src/**/*",
    "build": "npm run build:theme && npm run build:gui",
    "build:gui": "rollup -c",
    "build:theme": "node scripts/build-ui.js",
    "watch:gui": "rollup -c -w",
    "start": "electron main.js"
  },
  "dependencies": {
    "@ibm/plex": "^6.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "adm-zip": "^0.5.9",
    "axios": "^0.26.0",
    "carbon-components": "^10.53.0",
    "carbon-components-svelte": "^0.59.0",
    "carbon-icons-svelte": "^10.44.4",
    "electron": "^17.0.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.68.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "sass": "^1.49.8",
    "svelte": "^3.46.4",
    "svelte-preprocess": "^4.10.3"
  }
}
